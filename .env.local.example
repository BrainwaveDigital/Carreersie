# Copy this file to .env.local and fill in real values (do NOT commit .env.local with secrets)

# Public (browser-safe) Supabase values
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-public-key

# Server-only (keep secret)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Optional LLM/OpenAI
OPENAI_API_KEY=
LLM_MODEL=gpt-5
USE_LLM=0

# Local dev options
PARSE_SYNC=0
PARSE_POLL_INTERVAL=5000
